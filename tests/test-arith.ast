function int multiple(int a, int b)
{
  return a * b;
}

function int divide(int a, int b)
{
  return a / b;
}

function int mod(int a, int b)
{
  return a % b;
}

function int exp(int a, int b)
{
  return a ^ b;
}


int a;
int b;
int c;
int d;
a = multiple(2, 3);
b = divide(9, 3);
c = mod(10, 3);
d = exp(3, 4);
print(a);
print(b);
print(c);
print(d);

